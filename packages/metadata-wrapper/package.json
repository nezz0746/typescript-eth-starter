{
  "name": "my-eth-starter-server",
  "version": "1.0.14",
  "author": "rjmacarthy",
  "private": false,
  "scripts": {
    "dev": "npm run build && concurrently --kill-others \"tsc -w -p src\" \"nodemon dist/index.js -e ts\"",
    "debug": "npm run build && concurrently --kill-others \"tsc -w -p src\" \"nodemon --inspect dist/index.js -e ts\"",
    "test:watch": "mocha -r ts-node/register -w ./spec/**/*.spec.ts --watch-extensions ts",
    "test": "mocha -r ts-node/register -w ./spec/**/*.spec.ts",
    "build": "tsc -p src",
    "start": "npm run build && node dist/index.js",
    "build:docker": "docker build -t rjmacarthy/express-typescript-starter .",
    "docker:compose": "docker-compose up",
    "format": "prettier --write 'src/**/*ts'",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:test": "tslint -c tslint.json 'spec/**/*.ts'",
    "format:test": "prettier --write 'spec/**/*ts'"
  },
  "dependencies": {
    "@ceramicnetwork/http-client": "^1.5.7",
    "@ceramicnetwork/stream-tile": "^1.5.7",
    "body-parser": "^1.13.3",
    "chroma-js": "^2.4.2",
    "cookie-parser": "~1.3.5",
    "cors": "^2.8.5",
    "cross-env": "^5.2.1",
    "debug": "^4.3.1",
    "dids": "^2.4.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "fresh": "^0.5.2",
    "glob": "^5.0.14",
    "ipfs-core": "^0.14.1",
    "ipfs-http-client": "^56.0.1",
    "key-did-provider-ed25519": "^1.1.0",
    "key-did-resolver": "^1.4.4",
    "lodash": "^4.17.21",
    "minimist": "^1.2.5",
    "mongoose": "^6.1.3",
    "morgan": "^1.10.0",
    "negotiator": "^0.6.1",
    "node-cron": "^3.0.0",
    "node-html-to-image": "^3.2.3",
    "pug": "^3.0.2",
    "serve-favicon": "^2.5.0",
    "socket.io": "^2.4.1",
    "tslint-config-prettier": "^1.18.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.12",
    "@types/lodash": "^4.14.170",
    "@types/mocha": "^5.2.0",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.4",
    "@types/node-cron": "^3.0.1",
    "@types/sinon": "^4.3.3",
    "@types/socket.io": "^2.1.13",
    "chai": "^4.3.4",
    "concurrently": "^3.5.0",
    "mocha": "^5.1.1",
    "node-mocks-http": "^1.10.1",
    "nodemon": "^2.0.7",
    "prettier": "^1.19.1",
    "sinon": "^5.0.7",
    "ts-node": "^6.0.3",
    "tslint": "^5.20.1",
    "typescript": "^3.9.9"
  }
}